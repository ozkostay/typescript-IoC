<%- include('./port/layout-start', {title: title}) %>
<form
  class="create-form bb"
  action="/api/books/"
  method="POST"
  enctype="multipart/form-data"
>
  <span>ID</span>
  <input type="text" readonly name="id" value="<%= item._id %>" />
  <span>Название</span>
  <input type="text" name="title" value="<%= item.title %>" />
  <span>Описание</span>
  <input type="text" name="description" value="<%= item.description %>" />
  <span>Автор</span>
  <input type="text" name="authors" value="<%= item.authors %>" />
  <span>favorite</span>
  <input type="text" name="favorite" value="<%= item.favorite %>" />
  <span>Обложка</span>
  <input type="text" name="fileCover" value="<%= item.fileCover %>" />
  <span>Файл книги</span>
  <input type="file" name="fileBook" value="2234343" />
  <button>Изменить</button>
</form>
<%- include('./port/layout-end') %>
